  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
  <script type="text/javascript">
  $(docuemnt).ready(function(){
      function removeBlanks(arr) {
          var split_arr = arr.split(" ");
          return split_arr.join("");
      }

      function acronyms(arr){
          var split_arr = arr.split(" ");
          var first_letter = [];
          for (var i = 0; i < split_arr.length(); i++){
              first_letter.push(split_arr[i]][0]);
          }
          return first_letter.join("");
      }

      function getDigits(str){
          num_lst = [];
          for (var i = 0; i < str.length(); i++){
              if (var[i]) == 1 || var[i] == 2 || var[i] == 3 || var[i] == 4 || var[i] == 5 || var[i] == 6 || var[i] == 7 || var[i] == 8 || var[i] == 9 || var[i] == 0){
                  num_lst.push(var[i]);
              }
          }
          return number(num_lst.join(""));
      }
    // ... your assignment here
  });
  </script>
